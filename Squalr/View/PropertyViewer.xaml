<UserControl
    x:Class="Squalr.View.PropertyViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="SqualrPropertyViewer"
    d:DesignHeight="256"
    d:DesignWidth="256"
    DataContext="{Binding PropertyViewerViewModel, Source={StaticResource ViewModelLocator}}"
    mc:Ignorable="d">
    <Grid Background="{StaticResource SqualrColorBlack}">
        <Grid.RowDefinitions>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Border>
            <Grid x:Name="propertyViewer" />
        </Border>
    </Grid>
</UserControl>